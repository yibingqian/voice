var DeepAffects = require("deep-affects");
var defaultClient = DeepAffects.ApiClient.instance;

// Configure API key authorization: UserSecurity
var UserSecurity = defaultClient.authentications["UserSecurity"];
UserSecurity.apiKey = "miRfAncdRwMfpmDjfrqU1tlBHpLRjAEE";

var apiInstance = new DeepAffects.DenoiseApi();

var body = DeepAffects.Audio.fromFile("/Users/apple/Desktop/1.m4a"); // {Audio} Audio object

var callback = function(error, data, response) {
  if (error) {
    console.error(error);
  } else {
    console.log("API called successfully. Returned data: " + data);
  }
};

// sync request
apiInstance.syncDenoiseAudio(body, callback);

webhook = "http://your/webhook/";
// async request
apiInstance.asyncDenoiseAudio(body, webhook, callback);
